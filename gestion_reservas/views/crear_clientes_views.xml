<odoo>
    <record id="view_coche_form" model="ir.ui.view">
        <field name="name">view.coche.form</field>
        <field name="model">coche</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="description"/>
                        <field name="active"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <menuitem id="coche_menu" parent="" name="coche_menu" action="action_view_coche"/>
</odoo>
