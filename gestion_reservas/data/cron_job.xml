<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="ir_cron_cancelar_reservas_pendientes" model="ir.cron">
      <field name="name">Cancelar reservas pendientes</field>
      <field name="model_id" ref="model_res_booking"/>
      <field name="state">code</field>
      <field name="code">model.cancelar_reservas_pendientes()</field>
      <field name="interval_number">1</field>
      <field name="interval_type">hours</field>
      <field name="nextcall" eval="(fields.Datetime.now() + datetime.timedelta(hours=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
      <field name="numbercall">-1</field>
      <field name="active" eval="True"/>
    </record>
  </data>
</odoo>
